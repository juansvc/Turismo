<h1>Listing transportes</h1>

<table>
  <thead>
    <tr>
      <th>Lugar salida</th>
      <th>Lugar llegada</th>
      <th>Hora salida</th>
      <th>Hora llegada</th>
      <th>Fecha</th>
      <th>Nombre transporte</th>
      <th>Nombre transporte</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transportes.each do |transporte| %>
      <tr>
        <td><%= transporte.lugar_salida %></td>
        <td><%= transporte.lugar_llegada %></td>
        <td><%= transporte.hora_salida %></td>
        <td><%= transporte.hora_llegada %></td>
        <td><%= transporte.fecha %></td>
        <td><%= transporte.nombre_transporte %></td>
        <td><%= transporte.nombre_transporte_id %></td>
        <td><%= link_to 'Show', transporte %></td>
        <td><%= link_to 'Edit', edit_transporte_path(transporte) %></td>
        <td><%= link_to 'Destroy', transporte, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Transporte', new_transporte_path %>
