<h1>Listing tipo_lugars</h1>

<table>
  <thead>
    <tr>
      <th>Nombre tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_lugars.each do |tipo_lugar| %>
      <tr>
        <td><%= tipo_lugar.nombre_tipo %></td>
        <td><%= link_to 'Show', tipo_lugar %></td>
        <td><%= link_to 'Edit', edit_tipo_lugar_path(tipo_lugar) %></td>
        <td><%= link_to 'Destroy', tipo_lugar, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo lugar', new_tipo_lugar_path %>
