<h1>Listing nombre_transportes</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nombre_transportes.each do |nombre_transporte| %>
      <tr>
        <td><%= nombre_transporte.nombre %></td>
        <td><%= link_to 'Show', nombre_transporte %></td>
        <td><%= link_to 'Edit', edit_nombre_transporte_path(nombre_transporte) %></td>
        <td><%= link_to 'Destroy', nombre_transporte, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nombre transporte', new_nombre_transporte_path %>
