<h1>Listing ubicacions</h1>

<table>
  <thead>
    <tr>
      <th>Longitud</th>
      <th>Latitud</th>
      <th>Referencia</th>
      <th>Ciudad</th>
      <th>Ciudad</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ubicacions.each do |ubicacion| %>
      <tr>
        <td><%= ubicacion.longitud %></td>
        <td><%= ubicacion.latitud %></td>
        <td><%= ubicacion.referencia %></td>
        <td><%= ubicacion.ciudad %></td>
        <td><%= ubicacion.ciudad_id %></td>
        <td><%= link_to 'Show', ubicacion %></td>
        <td><%= link_to 'Edit', edit_ubicacion_path(ubicacion) %></td>
        <td><%= link_to 'Destroy', ubicacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ubicacion', new_ubicacion_path %>
